pipeline {
  agent { label 'macos' }
  options {
    buildDiscarder(logRotator(numToKeepStr: '5'))
  }
  stages {
    stage('Tooling versions') {
      steps {
        sh '''
          docker context use default
          copilot --version
          docker --version
          docker compose version
        '''
      }
    }
  }
}
